  boolean manual;
  sector_t* sec;
  unsigned value = (unsigned)line->special - 0x3400;



  int Targ = (value & 0x0300) >> 8;
  int Dely = (value & 0x00c0) >> 6;
  int Sped = (value & 0x0018) >> 3;
  int Trig = (value & 0x0007) >> 0;

  secnum = -1;
  rtn = 0;



  if (Targ==LnF2HnF)
    P_ActivateInStasis(line->tag);


  manual = 0;
  if (Trig==PushOnce || Trig==PushMany)
  {
    if (!(sec = line->backsector))
      return rtn;
    secnum = sec-sectors;
    manual = 1;
    goto manual_lift;
  }


  while ((secnum = P_FindSectorFromLineTag(line,secnum)) >= 0)
  {
    sec = &sectors[secnum];

manual_lift:

    if (P_SectorActive(floor_special,sec))
    {
      if (!manual)
        continue;
      else
        return rtn;
    }


    rtn = 1;
    plat = Z_Malloc( sizeof(*plat), PU_LEVSPEC, 0);
    memset(plat, 0, sizeof(*plat));
    P_AddThinker(&plat->thinker);

    plat->sector = sec;
    plat->sector->floordata = plat;
    plat->thinker.function = T_PlatRaise;
    plat->crush = 0;
    plat->tag = line->tag;

    plat->type = genLift;
    plat->high = sec->floorheight;
    plat->status = down;


