static boolean st_stopped = 1;

void ST_Start(void)
{
  if (!st_stopped)
    ST_Stop();
  ST_initData();
  ST_createWidgets();
  st_stopped = 0;
}

static void ST_Stop(void)
{
  if (st_stopped)
    return;
  V_SetPalette(0);
  st_stopped = 1;
}

void ST_Init(void)
{
  veryfirsttime = 0;
  ST_loadData();
}
